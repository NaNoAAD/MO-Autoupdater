<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mo.updating.controllers.ConfirmationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="50.0" prefWidth="613.0" style="&#10;" text="Se ha encontrado una nueva versión de Multimodal Observer!" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextArea fx:id="newVersionNotes" layoutX="35.0" layoutY="64.0" prefHeight="200.0" prefWidth="571.0"/>
            <Label alignment="CENTER" layoutX="14.0" layoutY="270.0" prefHeight="50.0" prefWidth="613.0" style="&#10;" text="¿Desea descargar y aplicar la nueva versión?" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="yesButton" onAction="#acceptUpdate" layoutX="181.0" layoutY="329.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="114.0" text="Si" />
            <Button fx:id="noButton" onAction="#cancelUpdate" layoutX="336.0" layoutY="329.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="114.0" text="No" />
      </children>
    </AnchorPane>
  </children>
</VBox>
